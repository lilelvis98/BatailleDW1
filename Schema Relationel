Joueur(@Id_Joueur, Nom_Joueur, Prenom_Joueur, Date_derniere_co, Sexe, Date_Naissance, Ville_Residence, Pseudo, Mdp)

Partie(@Id_Partie, #Id_Initiateur, #Id_Invite, #Id_etat, #Id_Gagnant, Date_Creation)

Etat_Partie(@Id_etat, libelle_etat)

Bateau(@Id_bateau, @#Id_Partie, @#Id_Joueur, @Id_Type_Navire, Coord_Y, Coord_X, Id_Orientation, Etat)

Type_Orientation(@Id_Orientation, libelle_Orientation)

Type_Navire(@Id_navire, libelle_navire, Lien, Nb_Cases)

Tour(@Id_Tour, @#Id_Partie, #Id_Joueur, #Id_Carte)

Carte(@Id_Carte, Nom_Carte, Probabilite, Rare)

Tir(@#Id_Carte, @Coord_Y, @Coord_X, Touche)
